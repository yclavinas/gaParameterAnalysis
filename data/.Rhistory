plotforKGA = function(data, f_interval, algo, dim){
aux <- data[f %in% c(f_interval)]
aux <- data[k %in% c(2)]
min = tapply(aux$min, INDEX = aux$gen, FUN = mean)
plot_d <- data.frame(min)
